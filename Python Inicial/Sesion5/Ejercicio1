
def triangleArea(base, height, c):

    print("El area del triángulo cuya base es", base, "y altura es", height, "es de:", (base * height) / c)



triangleArea(10, 5, 2)

def circleArea(radius, pi):

    print("El area del círculo con radio", radius, "es de:", pi *(radius ** 2))

circleArea(10, 3.14)
